#!/bin/bash
GIT_VERSION='$VERSION: [v4.3-8] $'
GIT_VERSION=`echo $GIT_VERSION| awk '{print $2}'|sed 's/\[//g'|sed 's/\]//g' `
echo "VERSION now=>$GIT_VERSION<"

